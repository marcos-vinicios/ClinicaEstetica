        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.TelaFinalActivity"></activity>
        <activity android:name=".activity.SugestaoActivity" />
        <activity android:name=".activity.QuartaActivity" />
        <activity android:name=".activity.TerceiraActivity" />
        <activity android:name=".activity.SegundaActivity" />
        <activity android:name=".activity.PrimeiraActivity" />
        <activity android:name=".activity.TelaSplashActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>